
#include <stdio.h>

int main() {
    int rows;

    printf("Enter number of rows: ");
    scanf("%d", &rows);

    for (int i = 0; i < rows; i++) {
        int value = 1;

        for (int j = 0; j <= i; j++) {
            printf("%d ", value);
            value = value * (i -6 j) / (j + 1);
        }
        printf("\n");
    }

    return 0;
}

